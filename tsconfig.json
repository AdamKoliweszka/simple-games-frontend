{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "cards": [
        "dist/cards"
      ],
      "cards/*": [
        "dist/cards/*"
      ],
      "card-war": [
        "dist/card-war"
      ],
      "card-war/*": [
        "dist/card-war/*"
      ],
      "users": [
        "dist/users"
      ],
      "users/*": [
        "dist/users/*"
      ],
      "game-turns": [
        "dist/game-turns"
      ],
      "game-turns/*": [
        "dist/game-turns/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}