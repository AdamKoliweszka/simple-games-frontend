{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "cards": {
      "projectType": "library",
      "root": "projects/cards",
      "sourceRoot": "projects/cards/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/cards/tsconfig.lib.json",
            "project": "projects/cards/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/cards/src/test.ts",
            "tsConfig": "projects/cards/tsconfig.spec.json",
            "karmaConfig": "projects/cards/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/cards/tsconfig.lib.json",
              "projects/cards/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "card-war": {
      "projectType": "library",
      "root": "projects/card-war",
      "sourceRoot": "projects/card-war/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/card-war/tsconfig.lib.json",
            "project": "projects/card-war/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/card-war/src/test.ts",
            "tsConfig": "projects/card-war/tsconfig.spec.json",
            "karmaConfig": "projects/card-war/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/card-war/tsconfig.lib.json",
              "projects/card-war/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "users": {
      "projectType": "library",
      "root": "projects/users",
      "sourceRoot": "projects/users/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/users/tsconfig.lib.json",
            "project": "projects/users/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/users/src/test.ts",
            "tsConfig": "projects/users/tsconfig.spec.json",
            "karmaConfig": "projects/users/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/users/tsconfig.lib.json",
              "projects/users/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "gameTurns": {
      "projectType": "library",
      "root": "projects/game-turns",
      "sourceRoot": "projects/game-turns/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/game-turns/tsconfig.lib.json",
            "project": "projects/game-turns/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/game-turns/src/test.ts",
            "tsConfig": "projects/game-turns/tsconfig.spec.json",
            "karmaConfig": "projects/game-turns/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/game-turns/tsconfig.lib.json",
              "projects/game-turns/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "cardGamesOffline": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/cardGamesOffline",
      "sourceRoot": "projects/cardGamesOffline/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/cardGamesOffline",
            "index": "projects/cardGamesOffline/src/index.html",
            "main": "projects/cardGamesOffline/src/main.ts",
            "polyfills": "projects/cardGamesOffline/src/polyfills.ts",
            "tsConfig": "projects/cardGamesOffline/tsconfig.app.json",
            "aot": false,
            "assets": [
              "projects/cardGamesOffline/src/favicon.ico",
              "projects/cardGamesOffline/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "projects/cardGamesOffline/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/cardGamesOffline/src/environments/environment.ts",
                  "with": "projects/cardGamesOffline/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "cardGamesOffline:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "cardGamesOffline:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "cardGamesOffline:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/cardGamesOffline/src/test.ts",
            "polyfills": "projects/cardGamesOffline/src/polyfills.ts",
            "tsConfig": "projects/cardGamesOffline/tsconfig.spec.json",
            "karmaConfig": "projects/cardGamesOffline/karma.conf.js",
            "assets": [
              "projects/cardGamesOffline/src/favicon.ico",
              "projects/cardGamesOffline/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "projects/cardGamesOffline/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/cardGamesOffline/tsconfig.app.json",
              "projects/cardGamesOffline/tsconfig.spec.json",
              "projects/cardGamesOffline/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/cardGamesOffline/e2e/protractor.conf.js",
            "devServerTarget": "cardGamesOffline:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "cardGamesOffline:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "cards",
  "schematics": {
    "@schematics/angular:component": {
      "styleext": "scss"
    }
  }
}